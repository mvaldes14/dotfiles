{
  "blog post": {
    "prefix": "bpm",
    "body": [
      "---\ntitle: ${1}\n date: ${2}\n description:${3}\n status: Draft\n ---\n "
    ],
    "description": "New post metadata header"
  },
  "todo items": {
    "prefix": "tdi",
    "body": [
      "- [ ] ${1}\n- [ ] ${2}\n- [ ] ${3}"
    ],
    "description": "New todo item"
  },
  "insert code block": {
    "prefix": "icb",
    "body": [
      "```${1}\n${2}\n```"
    ],
    "description": "Insert code block"
  },
  "new obsidian document": {
    "prefix": "nod",
    "body": [
      "---\nname:${1}\ntags: ${2}\ntopics: ${3}\n---"
    ],
    "description": "New obsidian document"
  }
}
